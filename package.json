{
  "name": "mongoose.do",
  "version": "0.1.1",
  "description": "Mongoose-compatible ODM for Cloudflare Durable Objects with TypeScript-first $ API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format esm --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src test",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test:run"
  },
  "keywords": [
    "mongoose",
    "mongodb",
    "odm",
    "cloudflare",
    "durable-objects",
    "workers",
    "typescript",
    "zod",
    "schema",
    "validation"
  ],
  "author": "Nathan Clevenger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dot-do/mongoose.git"
  },
  "bugs": {
    "url": "https://github.com/dot-do/mongoose/issues"
  },
  "homepage": "https://github.com/dot-do/mongoose#readme",
  "peerDependencies": {
    "mongo.do": ">=0.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false
}
